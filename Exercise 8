import numpy as np
from numpy import ndarray

A = np.array([[1, 2], [3, 4]])
B = np.array([[-1, 1], [5, 7]])
detA = np.linalg.det(A)
detB = np.linalg.det(B)

print("\nDeterminant of given matrix:")
print(int(detA))
print(int(detB))

detAB = np.dot(A,B)
print("\nProduct of the numbers:")
print(detAB)
